/mypage'>\n                            <MyPage />\n                        </PrivateRoute>\n                        <PrivateRoute exact path='/chat'>\n                            <ChatPage />\n                        </PrivateRoute>\n                        <PrivateRoute exact path='/post'>\n                            <PostPage />\n                        </PrivateRoute>\n                        <PrivateRoute exact path='/recipients' >\n                            <RecipientsPage />\n                        </PrivateRoute>\n                        <PrivateRoute exact path='/sender' >\n                            <SenderPage />\n                        </PrivateRoute>\n                    </Switch>\n                </BrowserRouter>\n            </ThemeProvider>\n        </>\n    );\n});\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);"]}}